<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    
    
    <context:annotation-config/> 
  <bean id="product" class="com.spring.util.Product" > 
  		<property name = "prodName" value = "prod12234"></property>
  		<property name = "price" value = "200"></property>
  </bean>
  <bean id="order" class="com.spring.util.Order" autowire="constructor"> 
  		<property name = "orderId" value = "2514"></property>
  		<property name = "qty" value = "20"></property>
  </bean>
  </beans>