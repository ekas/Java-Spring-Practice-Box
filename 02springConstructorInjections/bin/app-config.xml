<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id="orderBean" class="com.spring.inventory.Order">
    	<constructor-arg name = "orderName" value = "Herbal Products"></constructor-arg>
    	<constructor-arg name = "productName" value = "Herbal Oil"></constructor-arg>
    	<constructor-arg name = "qty" value = "10"></constructor-arg>
    	<constructor-arg name = "price" value = "200"></constructor-arg>
    </bean>
</beans>